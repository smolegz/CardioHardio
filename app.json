{
  "expo": {
    "name": "CardioHardio",
    "slug": "CardioHardio",
    "owner": "ivan.koo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#212A3E"
    },
    "updates": {
      
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.CardioHardio.CardioHardio"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.CardioHardio"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "Allow CardioHardio to access your camera."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "075cc32b-fba1-4aa1-8390-7b879607e534"
      }
    }
  }
}
